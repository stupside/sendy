    "eslint:check": "pnpm -r exec eslint --cache ."
  },
  "dependencies": {
    "husky": "^9.0.11",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0"
  },
  "engines": {
    "npm": "use pnpm",
    "yarn": "use pnpm"
  }
}